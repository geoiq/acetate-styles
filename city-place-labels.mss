#null-island-label[zoom>=9] name
{
    text-face-name: 'Arial Bold';
    text-wrap-width: 80;
    text-size: 18;
    text-fill: #b4b4b4;
    text-halo-radius: 1;
    text-halo-fill: #e4e3db;
}

#null-island-label[zoom=12] name { text-dy: 3; }
#null-island-label[zoom=13] name { text-dy: 6; }
#null-island-label[zoom=14] name { text-dy: 9; }
#null-island-label[zoom=15] name { text-dy: 12; }
#null-island-label[zoom=16] name { text-dy: 15; }
#null-island-label[zoom=17] name { text-dy: 25; }
